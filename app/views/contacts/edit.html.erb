
<%= simple_form_for (@contact) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :emails %>
    <%= f.input :cellphone %>
  </div>

  <div class="form-actions">
     <%= f.submit "add a contact", class: "btn btn-primary" %>
  </div>
<% end %>